{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport Parser from 'rss-parser';\nexport default {\n  data() {\n    return {\n      newsSources: [{\n        id: 'google-news',\n        name: 'Google News',\n        rss: 'https://news.google.com/rss'\n      }, {\n        id: 'cnn',\n        name: 'CNN',\n        rss: 'http://rss.cnn.com/rss/cnn_topstories.rss'\n      }\n      // 可自行添加更多來源\n      ],\n      selectedSource: 'google-news',\n      news: [],\n      currentArticle: null\n    };\n  },\n  methods: {\n    async fetchNews() {\n      const parser = new Parser();\n      const source = this.newsSources.find(s => s.id === this.selectedSource);\n      try {\n        const feed = await parser.parseURL(source.rss);\n        this.news = feed.items;\n      } catch (error) {\n        console.error('獲取新聞失敗:', error);\n      }\n    },\n    showArticle(link) {\n      this.currentArticle = link;\n    }\n  },\n  mounted() {\n    this.fetchNews();\n  }\n};","map":{"version":3,"names":["Parser","data","newsSources","id","name","rss","selectedSource","news","currentArticle","methods","fetchNews","parser","source","find","s","feed","parseURL","items","error","console","showArticle","link","mounted"],"sources":["/home/bing/Git/quantum_news/quantum-news/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>新聞應用程式</h1>\n    <select v-model=\"selectedSource\" @change=\"fetchNews\">\n      <option v-for=\"source in newsSources\" :key=\"source.id\" :value=\"source.id\">\n        {{ source.name }}\n      </option>\n    </select>\n    <ul>\n      <li v-for=\"(article, index) in news\" :key=\"index\" @click=\"showArticle(article.link)\">\n        <h2>{{ article.title }}</h2>\n      </li>\n    </ul>\n    <iframe v-if=\"currentArticle\" :src=\"currentArticle\" width=\"100%\" height=\"600px\"></iframe>\n  </div>\n</template>\n\n<script>\nimport Parser from 'rss-parser';\n\nexport default {\n  data() {\n    r